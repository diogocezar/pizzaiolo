{
  "display_information": {
    "name": "<Your bot's display name>"
  },
  "features": {
    "bot_user": {
      "display_name": "<Your bot's display name>",
      "always_online": false
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "app_mentions:read",
        "channels:read",
